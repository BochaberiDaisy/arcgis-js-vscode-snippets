{
  "tsconfig": {
    "prefix": "tsConfig",
    "body": [
      "{",
      "\t\"compilerOptions\": {",
      "\t\t\"lib\": [",
      "\t\t\t\"es2015\"",
      "\t\t],",
      "\t\t\"module\": \"amd\",",
      "\t\t\"noImplicitAny\": true,",
      "\t\t\"sourceMap\": true,",
      "\t\t\"jsx\": \"react\",",
      "\t\t\"jsxFactory\": \"tsx\",",
      "\t\t\"target\": \"es5\",",
      "\t\t\"experimentalDecorators\": true,",
      "\t\t\"preserveConstEnums\": true,",
      "\t\t\"suppressImplicitAnyIndexErrors\": true,",
      "\t\t\"importHelpers\": true,",
      "\t\t\"moduleResolution\":\"node\"",
      "\t},",
      "\t\"include\": [",
      "\t\t\"./app/*\"",
      "    ],",
      "    \"exclude\": [",
      "        \"node_modules\"",
      "    ]",
      "}"
    ],
    "description": "TypeScript Configuration"
  },
  "tsconfig-basic": {
    "prefix": "tsConfigBasic",
    "body": [
      "{",
      "\t\"compilerOptions\": {",
      "\t\t\"module\": \"amd\",",
      "\t\t\"sourceMap\": true,",
      "\t\t\"target\": \"es2019\",",
      "\t\t\"esModuleInterop\": true",
      "\t}",
      "}"
    ],
    "description": "Simple TS Config"
  }
}
